{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false
        }
    },
    "rules": {
        "semi": 2,
        "no-unreachable": 2,
        "eol-last": 2,
        "no-console": 1,
        "linebreak-style": 1
    }
}